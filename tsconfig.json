{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
			"DOM",
			"ES6"
		],
		"types": [
			"vscode",
			"vscode-notebook-renderer",
			"node"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
